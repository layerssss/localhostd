{
  "name": "bnb",
  "version": "1.0.6",
  "description": "Bnb takes care of launching your web app on your development machine.",
  "main": "bin/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --watch lib --watch bin bin/bnb.js server --bind 0.0.0.0 --port 3000",
    "rebuild-app": "electron-builder install-app-deps",
    "app": "electron .",
    "dist": "build",
    "lint": "eslint ."
  },
  "bin": {
    "bnb": "bin/bnb.js"
  },
  "build": {
    "appId": "in.micy.bnb",
    "mac": {
      "category": "Development"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage",
        "rpm"
      ]
    },
    "win": {
      "target": [
        "squirrel"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/layerssss/bnb.git"
  },
  "author": {
    "name": "Michael Yin",
    "email": "layerssss@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/layerssss/bnb/issues"
  },
  "homepage": "https://github.com/layerssss/bnb#readme",
  "dependencies": {
    "commander": "^2.18.0",
    "death": "^1.1.0",
    "electron-builder-squirrel-windows": "^20.28.3",
    "express": "^4.16.3",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "node-forge": "0.7.6",
    "node-netstat": "^1.4.2",
    "node-pty": "0.7.7",
    "portfinder": "^1.0.17",
    "shell-env": "^2.1.0",
    "shell-quote": "^1.6.1",
    "strip-ansi": "^4.0.0",
    "update-notifier": "^2.5.0",
    "username": "^4.0.0",
    "which": "^1.3.1",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "electron": "^3.0.0",
    "electron-builder": "^20.28.4",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.0",
    "utf-8-validate": "^5.0.1"
  }
}
