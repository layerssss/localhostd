{
  "name": "localhostd",
  "version": "1.0.29",
  "description": "LocalhostD takes care of launching your web app on your development machine.",
  "main": "bin/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "ui_watch": "LOCALHOSTD_DEV_WATCH=yes node ./bin/ui_build.js",
    "ui_build": "node ./bin/ui_build.js",
    "node_dev": "nodemon --watch lib --watch bin bin/cli.js server --debug --bind 0.0.0.0",
    "app_deps": "electron-rebuild",
    "app_dev": "electron .",
    "app_build": "electron-builder build",
    "build": "yarn ui_build && yarn app_build",
    "dist": "yarn ui_build && yarn app_build",
    "lint": "eslint .",
    "dev": "concurrently \"yarn ui_watch\" \"yarn node_dev\"",
    "cleanup": "rm -Rf ./ui_build ./dist"
  },
  "bin": {
    "localhostd": "bin/cli.js"
  },
  "homepage": "https://github.com/layerssss/localhostd",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "in.micy.localhostd",
    "productName": "LocalhostD",
    "extends": null,
    "mac": {
      "category": "Development"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage",
        "rpm"
      ],
      "category": "Development"
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/layerssss/localhostd.git"
  },
  "author": {
    "name": "Michael Yin",
    "email": "layerssss@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/layerssss/localhostd/issues"
  },
  "dependencies": {
    "app-builder-lib": "^20.28.3",
    "commander": "^2.18.0",
    "default-shell": "^1.0.1",
    "electron-updater": "^4.0.5",
    "execa": "^1.0.0",
    "express": "^4.16.3",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "node-forge": "0.7.6",
    "node-pty": "^0.8.1",
    "portfinder": "^1.0.28",
    "shell-quote": "^1.6.1",
    "strip-ansi": "^5.0.0",
    "update-notifier": "^2.5.0",
    "username": "^4.0.0",
    "which": "^1.3.1",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "babel-eslint": "9.x",
    "bootswatch": "3",
    "concurrently": "^4.0.1",
    "electron": "^4.0.0",
    "electron-builder": "^20.28.4",
    "electron-rebuild": "^1.8.2",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-app": "^3.0.3",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "font-awesome": "4",
    "is-electron": "^2.1.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.2",
    "react-event-listener": "^0.6.4",
    "react-scripts": "^2.0.4",
    "react-toastify": "^4.4.0",
    "recompose": "^0.30.0",
    "uuid": "^3.3.2",
    "xterm": "^3.8.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.0",
    "utf-8-validate": "^5.0.1"
  }
}
